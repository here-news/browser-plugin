{
    "manifest_version": 3,
    "name": "Newstr Extension",
    "version": "1.0.4",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "storage"
        ],
    "host_permissions": [
        "*://*/*"
    ]
}

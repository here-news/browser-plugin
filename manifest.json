{
    "manifest_version": 3,
    "name": "Newstr Extension",
    "version": "1.5.0",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "img/cool.png"
        }
    },
    "options_page": "options.html",

    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "storage"
        ],
    "host_permissions": [
        "*://*/*"
    ]
}
